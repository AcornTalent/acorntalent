extends layout

block content

  p
  h1='Results for ' + test.name
  p.lead=test.description
  hr
  if(!test || !user)
    p User or test does not exist! Please check back later or contact us at support@acorntalent.io
  else
    p='Your score for this test was ' + user.score + '/' + test.questionsID.length + ' (' + (parseInt(user.score)/parseInt(test.questionsID.length)*100) + '%)'
  a.btn.btn-raised(href='/dashboard', role='button') Back to Dashboard Â»
  p
